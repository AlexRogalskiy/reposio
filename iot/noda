You can control 8 lamps by doing the following
- Connect nodeMCU to 8 channel relay module in D1,D2,D4,D5,D6,D7,D9 and D10 and vin and Ground to VCC and ground of the realy then

- Upload below code to your NodeMCU ESP8266 and then change x1 and x2 to your wifi ssid and password the x3 and x4 to this mobile app user name and password and x5 to your selected device name

#include <ESP8266WiFi.h>
#include <ESP8266HTTPClient.h>
#include <ArduinoJson.h>
const char* wifiName = "x1";
const char* wifiPassword = "x2";
const String mobileAppUserName="x3";
const String mobileAppPassword="x4";
const String deviceName="x5";

pinMode(5, OUTPUT);
pinMode(12, OUTPUT);
pinMode(13, OUTPUT);
pinMode(14, OUTPUT);
pinMode(1, OUTPUT);
}
void loop(){
Serial.println("test");
if (WiFi.status() == WL_CONNECTED)
{
HTTPClient http; //Object of class HTTPClient
http.begin("http://iot.somee.com/api/switch/"+mobileAppUserName+"/"+deviceName);
int httpCode = http.GET();
if (httpCode > 0)
{
const size_t capacity = JSON_OBJECT_SIZE(17) + 140;
DynamicJsonDocument doc(capacity);
auto error = deserializeJson(doc, http.getString());
if (error) {
Serial.print(F("deserializeJson() failed with code "));
Serial.println(error.c_str());
return;
}
const char* Pin0 = doc["Pin0"];
const char* Pin1 = doc["Pin1"];
const char* Pin2 = doc["Pin2"];
const char* Pin3 = doc["Pin3"];
const char* Pin4 = doc["Pin4"];
const char* Pin5 = doc["Pin5"];
const char* Pin6 = doc["Pin6"];
const char* Pin7 = doc["Pin7"];
const char* Pin8 = doc["Pin8"];
const char* Pin9 = doc["Pin9"];
const char* Pin10 = doc["Pin10"];
const char* Pin11 = doc["Pin11"];
const char* Pin12 = doc["Pin12"];
const char* Pin13 = doc["Pin13"];
const char* Pin14 = doc["Pin14"];
const char* Pin15 = doc["Pin15"];
const char* Pin16 = doc["Pin16"];
if (strcmp(Pin1, "1") == 0)
{
Serial.println("High");
digitalWrite(1,HIGH);
}
else if (strcmp(Pin1, "0") == 0)
{
Serial.println("Low");
digitalWrite(1,LOW);
}
if (strcmp(Pin5, "1") == 0)
{
Serial.println("High");
digitalWrite(5,HIGH);
}
else if (strcmp(Pin5, "0") == 0)
{
Serial.println("Low");
digitalWrite(5,LOW);
}
if (strcmp(Pin4, "1") == 0)
{
Serial.println("High");
digitalWrite(4,HIGH);
}
else if (strcmp(Pin4, "0") == 0)
{
Serial.println("Low");
digitalWrite(4,LOW);
}
if (strcmp(Pin3, "1") == 0)
{
Serial.println("High");
digitalWrite(3,HIGH);
}
else if (strcmp(Pin3, "0") == 0)
{
Serial.println("Low");
digitalWrite(3,LOW);
}
if (strcmp(Pin2, "1") == 0)
{
Serial.println("High");
digitalWrite(2,HIGH);
}
else if (strcmp(Pin2, "0") == 0)
{
Serial.println("Low");
digitalWrite(2,LOW);
}
if (strcmp(Pin14, "1") == 0)
{
Serial.println("High");
digitalWrite(14,HIGH);
}
else if (strcmp(Pin14, "0") == 0)
{
Serial.println("Low");
digitalWrite(14,LOW);
}
if (strcmp(Pin12, "1") == 0)
{
Serial.println("High");
digitalWrite(12,HIGH);
}
else if (strcmp(Pin12, "0") == 0)
{
Serial.println("Low");
digitalWrite(12,LOW);
}
if (strcmp(Pin13, "1") == 0)
{
Serial.println("High");
digitalWrite(13,HIGH);
}
else if (strcmp(Pin13, "0") == 0)
{
Serial.println("Low");
digitalWrite(13,LOW);
}
http.end(); //Close connection
}
delay(2000);
}}

